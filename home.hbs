{{!< default}} <div class="content-area">
    <main class="site-main">
        {{#page}}
        <article class="post-content no-image single-post-homepage">
            {{> post-header single=true number_date=true big_title=true}}
            {{> post-media ratio="horizontal"}}
            <div class="post-content kg-canvas u-text-format">
                {{content}}
            </div>
        </article>
        <article class="post-content {{post_class}}{{#unless feature_image}} no-image{{/unless}} single-post"></article>
        {{/page}}
    </main>
    </div>